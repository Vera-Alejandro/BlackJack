<Window x:Class="BlackJack.v3.Profile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlackJack.v3"
        mc:Ignorable="d"
        Title="Profile" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
            <Button x:Name="MainMenu" Content="Main Menu" Height="30" Width="100" Margin="10 0" Click="MainMenu_OnClick"/>
            <Button x:Name="SignUp" Content="Sign Up" Height="30" Width="100" Margin="10 0 " Click="SignUp_OnClick"/>
            <Button x:Name="AddFunds" Content="Add Funds" Height="30" Width="100" Margin="10 0 " Click="AddFunds_OnClick"/>
            <Button x:Name="LogIn" Content="Log In" Height="30" Width="100" Margin="10 0" Click="LogIn_OnClick"/>

        </StackPanel>
    </Grid>
</Window>
